<h1> Your posts: </h1>
<% %>
<% @posts.each do |post| %>
  <%= User.find(post.user_id) %>
  <p><%= simple_format post.message %></p>
  <%= post.created_at.strftime("%d %B %Y %H:%M") %>
  <p><%= link_to 'Delete', post, method: :delete %></p>
<% end %>
</br>
</br>
<%= link_to new_user_post_path do %>
  New post
<% end %>
